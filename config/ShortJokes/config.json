{
    "random_seed": 21016,
    "pytorch_seed": 2335,
    "numpy_seed": 7005,
      "dataset_reader": {
          "type": "shortJokes_reader",
          "bert_model_name": "bert-base-uncased",
          "token_indexers": {
              "bert": {
                  "type": "bert-pretrained",
                  "pretrained_model": "bert-base-uncased"
              }
          }
      },
      "iterator": {
          "type": "bucket",
          "batch_size": 8,
          "sorting_keys": [
              [
                  "quote_response",
                  "num_tokens"
              ]
          ]
      },
      "model": {
          "type": "bert_classifier",
          "bert_model_name": "bert-base-uncased"
      },
      "train_data_path": "data/short_jokes/train.csv",
      "validation_data_path": "data/short_jokes/shortJokes_dev.csv",
      "test_data_path": "data/short_jokes/shortJokes_test.csv",
      "trainer": {
          "num_epochs": 30,
          "optimizer": {
              "type": "adagrad",
              "lr": 0.0001
          },
          "patience": 3,
          "validation_metric": "+average_F1"
      },
      "evaluate_on_test": true
  }
  